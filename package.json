{
  "name": "egg-joi-swagger",
  "version": "1.0.0",
  "description": "joi + swagger + validate plugin for eggjs",
  "eggPlugin": {
    "name": "joiSwagger"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "egg-validate",
    "egg-val",
    "egg-joi",
    "egg-swagger"
  ],
  "dependencies": {
    "joi": "^14.3.1",
    "joi2json": "^1.0.2",
    "lodash": "^4.17.11",
    "shins": "^2.3.2-4",
    "widdershins": "^3.6.7"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.16.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
